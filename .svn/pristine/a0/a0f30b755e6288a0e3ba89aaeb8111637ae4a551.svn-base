  <div id="server_stats" class="stats">
      <h2>Server 1</h2>
      <table class="stats_table" border="1">
          <tbody><tr><th>Metric</th> <th>Mean</th><th>Std Dev</th></tr>
          <tr><td>Arrivals</td><td id="arrival">-</td><td>-</td></tr>
          <tr><td>Dropped</td><td id="drop">-</td><td>-</td></tr>
          <tr><td>Server Utilization</td><td id="sutil">-</td><td>-</td></tr>
          <tr><td>Time spent in queue</td><td id="qtime">-</td><td id="qtimed">-</td></tr>
          <tr><td>Time spent in system</td><td id="stime">-</td><td id="stimed">-</td></tr>
          <tr><td>Length of queue</td><td id="qsize">-</td><td id="qsized">-</td></tr>
          <tr><td>Customers in system</td><td id="ssize">-</td><td id="ssized">-</td></tr>
      </tbody></table>
  </div>

  <div id="sink_stats" class="stats">
      <h2>Sink</h2>
      <table class="stats_table" border="1">
          <tbody><tr><th>Metric</th> <th>Mean</th><th>Std Dev</th></tr>
          <tr><td>Departures</td><td id="depart">-</td><td>-</td></tr>
          <tr><td>Population</td><td id="pop">-</td><td id="popd">-</td></tr>
          <tr><td>Stay duration</td><td id="stay">-</td><td id="stayd">-</td></tr>
      </tbody></table>
  </div>

  <div id="simulation_dialog" title="Simulation Properties" class="settings_form" style="display: none;">
      <button id="simulation_dialog_close" class="settings_form_close ui-button ui-widget ui-state-default ui-corner-all ui-button-icon-only" role="button" aria-disabled="false" title=""><span class="ui-button-icon-primary ui-icon ui-icon-close"></span><span class="ui-button-text"></span></button>
      <table>
          <tbody><tr><td><label for="sim_until">Simulation duration</label>
              <select id="time_selector" class="text ui-widget-content ui-corner-all" onChange="QueueApp.updateSettings();">
                  <option>hours</option>
                  <option>mins</option>
                  <option>secs</option>
              </select>
          </td>
          <td><input type="text" name="sim_until" class="text ui-widget-content ui-corner-all" value="5000" id="sim_until"></td></tr>
          <tr><td><label for="sim_seed">Random generation seed</label>

          </td>
          <td><input type="text" name="sim_seed" class="text ui-widget-content ui-corner-all" value="null" id="sim_seed"></td></tr>
      </tbody></table>
      <hr>
      <button id="simulation_dialog_save" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" role="button" aria-disabled="false"><span class="ui-button-text">Apply</span></button>
  </div>


  <div id="server_form" class="settings_form" style="display: none; position: absolute;">
      <button id="server_form_close" class="settings_form_close ui-button ui-widget ui-state-default ui-corner-all ui-button-icon-only" role="button" aria-disabled="false" title=""><span class="ui-button-icon-primary ui-icon ui-icon-close"></span><span class="ui-button-text"></span></button></br>
      <table>
              <tr>
